
<% @questions.each_key do |classid| %>

<owl:Class rdf:about="<%= @prefix %><%= classid %>">
<rdfs:subClassOf rdf:resource="<%= @prefix %><%= @questionnairetype %>"/>
<dc:description xml:lang="en"><%= @questions[classid] %></dc:description>
<dc:identifier>adc:<%= classid %></dc:identifier>
<rdfs:label xml:lang="en"><%= @questions[classid] %></rdfs:label>
</owl:Class>

<% end %>
